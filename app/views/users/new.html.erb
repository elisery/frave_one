<h1 class="ml-4">Sign Up</h1>
<div class="container-fluid bg-white ml-2 mt-3">
  <%= simple_form_for @user do |f| %>
   
    <%= f.input :first_name %>
    
    <%= f.input :last_name %>

    <%= f.input :email %>

    <%= f.input :password %>

    <%= f.input :password_confirmation %>

    <%= f.submit "Sign Up", class: "btn btn-outline-primary" %>
    
  <% end %>
</div>

